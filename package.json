{
  "name": "littlepioneer-js",
  "version": "1.0.0",
  "description": "littlepioneer cider house website using vanilla js",
  "main": "dist/bundle.js",
  "type": "module",
  "files": [
    "index-bundle.d.ts",
    "index-bundle.js",
    "index-bundle.js.map"
  ],
  "scripts": {
    "start": "tsc && rollup -c -w --bundleConfigAsCjs",
    "build": "tsc && rollup -c --bundleConfigAsCjs",
    "clean": "rm -rf ./src/bundle.js ./src/bundle.js.map",
    "deploy": "npm run build && npm run clean && gh-pages -d dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninjaofawesome/littlepioneer-js.git"
  },
  "keywords": [
    "javascript"
  ],
  "author": "Hannah Dresser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninjaofawesome/littlepioneer-js/issues"
  },
  "homepage": "https://github.com/ninjaofawesome/littlepioneer-js#readme",
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.11",
    "@web/rollup-plugin-html": "^2.1.2",
    "gh-pages": "^6.1.1",
    "rollup": "*",
    "rollup-plugin-sourcemaps": "^0.6.3"
  }
}
